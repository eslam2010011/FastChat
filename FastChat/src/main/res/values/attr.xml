<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MYLoader">
        <attr name="my_color" format="color"/>

    </declare-styleable>
    <declare-styleable name="GoogleMapView">
        <attr name="setMapType"   format="integer">
            <enum name="satellite" value="0" />
            <enum name="roadmap"   value="1" />
            <enum name="hybrid"    value="2" />
            <enum name="terrain"    value="3" />
        </attr>

        <attr name="setMapScale" format="integer">
            <enum name="low" value="1"/>
            <enum name="high" value="2"/>
        </attr>
        <attr name="setLatitude" format="float"/>
        <attr name="setLongitude" format="float"/>
        <attr name="setMapZoom"    format="integer"/>
        <attr name="setMapWidth" format="integer"/>
        <attr name="setMapHeight" format="integer"/>
    </declare-styleable>
    <declare-styleable name="AdCircleProgress">
        <attr name="adpgb_progress" format="float"/>
        <attr name="adpgb_max" format="integer"/>

        <attr name="adpgb_unfinished_color" format="color"/>

        <attr name="adpgb_finished_color" format="color"/>

        <attr name="adpgb_gradient_color_one" format="color"/>
        <attr name="adpgb_gradient_color_two" format="color"/>

        <attr name="adpgb_finished_stroke_width" format="dimension"/>
        <attr name="adpgb_unfinished_stroke_width" format="dimension"/>

        <attr name="adpgb_text_size" format="dimension"/>
        <attr name="adpgb_text_color" format="color"/>

        <attr name="adpgb_prefix_text" format="string"/>
        <attr name="adpgb_suffix_text" format="string"/>
        <attr name="adpgb_text" format="string"/>

        <attr name="adpgb_background_color" format="color"/>

        <attr name="adpgb_inner_bottom_text" format="string"/>
        <attr name="adpgb_inner_bottom_text_size" format="dimension"/>
        <attr name="adpgb_inner_bottom_text_color" format="color"/>

        <attr name="adpgb_circle_starting_degree" format="integer" />
        <attr name="adpgb_show_text" format="boolean"/>
        <attr name="adpgb_inner_drawable" format="reference"/>
    </declare-styleable>


    <declare-styleable name="Avatar">
        <attr name="avatar_shape" format="string" />
        <attr name="avatar" format="reference" />
        <attr name="avatar_initials" format="string" />
        <attr name="border_color" format="color" />
        <attr name="border_width" format="dimension" />


    </declare-styleable>

    <declare-styleable name="StatusIndicator">
        <attr name="user_status" format="string" />

    </declare-styleable>

    <declare-styleable name="CometChatUserList">
        <attr name="userlist" format="reference" />
        <attr name="headers" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="CometChatConversationList">
        <attr name="conversationlist" format="reference" />

    </declare-styleable>
    <declare-styleable name="CometChatGroupList">
        <attr name="grouplist" format="reference" />

    </declare-styleable>

    <declare-styleable name="SmartReplyList">
        <attr name="replylist" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SharedMediaView">
        <attr name="reciever_id" format="string"/>
        <attr name="reciever_type" format="string"/>
    </declare-styleable>

    <declare-styleable name="BadgeCount">
        <attr name="count" format="integer" />
        <attr name="count_size" format="dimension" />
        <attr name="count_color" format="color" />
        <attr name="count_background_color" format="color" />

    </declare-styleable>

    <declare-styleable name="ComposeBox">
        <attr name="color" format="color"/>
    </declare-styleable>





    <declare-styleable name="SwipeRevealLayout">
        <attr name="dragEdge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
            <flag name="top" value="4" />
            <flag name="bottom" value="8" />
        </attr>

        <attr name="mode">
            <enum name="normal" value="0" />
            <enum name="same_level"   value="1" />
        </attr>

        <attr name="flingVelocity" format="integer" />

        <attr name="minDistRequestDisallowParent" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RecordAudio">
        <attr name="slide_to_cancel_text" format="string" />
        <attr name="slide_to_cancel_arrow" format="reference" />
        <attr name="slide_to_cancel_margin_right" format="dimension" />
        <attr name="slide_to_cancel_bounds" format="dimension" />
        <attr name="slide_to_cancel_arrow_color" format="color" />
        <attr name="counter_time_color" format="color" />

    </declare-styleable>

    <declare-styleable name="RecordMic">

        <attr name="mic_icon" format="reference" />

    </declare-styleable>

    <declare-styleable name="AudioRecordView">

        <attr name="chunkWidth" format="dimension" />
        <attr name="chunkMaxHeight" format="dimension" />
        <attr name="chunkMinHeight" format="dimension" />
        <attr name="chunkSpace" format="dimension" />
        <attr name="chunkColor" format="color" />
        <attr name="chunkRoundedCorners" format="boolean" />
        <attr name="chunkAlignTo" format="enum">
            <enum name="center" value="1" />
            <enum name="bottom" value="2" />
        </attr>
        <attr name="chunkSoftTransition" format="boolean" />
    </declare-styleable>


    <declare-styleable name="WaveformSeekBar">
        <attr name="waveBackgroundColor" format="color"/>
        <attr name="waveProgressColor" format="color"/>
        <attr name="waveGap" format="dimension"/>
        <attr name="waveCornerRadius" format="dimension"/>
        <attr name="waveMaxWidth" format="dimension"/>
        <attr name="waveAnimDuration" format="integer"/>
    </declare-styleable>

    <attr name="waveformSeekBarStyle" format="reference"/>

    <declare-styleable name="TypingIndicatorView">
        <attr name="dotSize" format="dimension"/>
        <attr name="dotCount" format="integer"/>
        <attr name="dotColor" format="color"/>
        <attr name="dotSecondColor" format="color"/>
        <attr name="dotHorizontalSpacing" format="dimension"/>
        <attr name="dotMaxCompressRatio" format="fraction"/>
        <attr name="dotAnimationDuration" format="integer"/>
        <attr name="dotAnimationType" format="enum">
            <enum name="Grow" value="0"/>
            <enum name="Wink" value="1"/>
            <enum name="Disappear" value="2"/>
            <enum name="Sliding" value="3"/>
            <enum name="BouncingSliding" value="4"/>
        </attr>
        <attr name="showBackground" format="boolean"/>
        <attr name="backgroundColor" format="color"/>
        <attr name="backgroundType" format="enum">
            <enum name="square" value="0"/>
            <enum name="rounded" value="1"/>
        </attr>
        <attr name="animateFrequency" format="integer"/>
        <attr name="animationOrder" format="enum">
            <enum name="random" value="0"/>
            <enum name="sequence" value="1"/>
            <enum name="circular" value="2"/>
            <enum name="lastOneFirst" value="3"/>
        </attr>
    </declare-styleable>


</resources>
